{
  "i18n-ally.displayLanguage": "de",
  "i18n-ally.extract.keyMaxLength": 99999999,
  "i18n-ally.enabledParsers": ["json5"],
  "i18n-ally.pathMatcher": "{namespace}/{locale}.{ext}",
  "i18n-ally.localesPaths": [
    "resources/js/lang",
    "resources/lang"
  ],
  "i18n-ally.namespace": true,
  "i18n-ally.includeSubfolders": false,
  "i18n-ally.enabledFrameworks": ["vue", "vue-sfc"],
  "i18n-ally.extract.autoDetect": true,
  "i18n-ally.keystyle": "nested",
  "eslint.workingDirectories": ["resources/js/"],
  "eslint.options": {
    "overrideConfigFile": ".eslintrc.js"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "prettier.configPath": "./.prettierrc.json",
  "prettier.prettierPath": "./node_modules/prettier",
  "eslint.format.enable": true,
  "vs-code-prettier-eslint.prettierLast": true,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnSave": false
}
